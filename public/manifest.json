{
    "manifest_version": 3,
    "name": "My vite extensions.",
    "description": "Extension that includes a popup and requests host permissions and storage permissions .",
    "version": "1.0",
    "icons": {
      "32": "icon32.png"
      
    },
    "action": {
      "default_popup": "index.html"
    },
    "host_permissions": [
      "https://*/",
      "http://*/"
    ],
    "permissions": [
     "scripting"
    ]
  }